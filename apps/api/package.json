{
    "name": "api",
    "version": "1.0.0",
    "description": "Rent Manager API",
    "main": "dist/index.js",
    "scripts": {
        "dev": "dotenvx run -f ../../.env -f .env -- tsx watch src/index.ts",
        "build": "tsc",
        "start": "dotenvx run -f ../../.env -f ../../.env.production -- node dist/index.js",
        "token": "dotenvx run -f ../../.env -f .env -- tsx src/scripts/generate-token.ts",
        "typecheck": "tsc --noEmit",
        "migration:create": "mikro-orm migration:create",
        "migration:up": "mikro-orm migration:up",
        "migration:down": "mikro-orm migration:down",
        "migration:pending": "mikro-orm migration:pending",
        "migration:fresh": "mikro-orm migration:fresh",
        "create-superadmin": "dotenvx run -f ../../.env -f ../../.env.production -- node dist/scripts/create-super-admin.js",
        "create-superadmin:dev": "dotenvx run -f ../../.env -f .env -- tsx src/scripts/create-super-admin.ts",
        "test-renewals": "tsx test-renewals.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.964.0",
        "@aws-sdk/s3-request-presigner": "^3.964.0",
        "@dotenvx/dotenvx": "^1.51.4",
        "@mikro-orm/cli": "^6.6.3",
        "@mikro-orm/core": "^6.0.0",
        "@mikro-orm/migrations": "^6.0.0",
        "@mikro-orm/postgresql": "^6.0.0",
        "@mikro-orm/reflection": "^6.0.0",
        "@mikro-orm/seeder": "^6.0.0",
        "@proppio/shared": "file:../shared",
        "@types/multer": "^2.0.0",
        "@types/node-cron": "^3.0.11",
        "bcrypt": "^5.1.0",
        "bullmq": "^4.0.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.18.0",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.3",
        "multer": "^2.0.2",
        "node-cache": "^5.1.2",
        "node-cron": "^4.2.1",
        "pdfkit": "^0.17.2",
        "pg": "^8.16.3",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "puppeteer": "^24.34.0",
        "resend": "^6.7.0",
        "zod": "^3.21.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.0.0",
        "@types/node-cache": "^4.1.3",
        "@types/pdfkit": "^0.17.4",
        "@types/uuid": "^10.0.0",
        "globals": "^17.0.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.52.0"
    }
}
